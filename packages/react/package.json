{
  "name": "@igortullio-ui/react",
  "version": "2.0.9",
  "description": "React component lib",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "build:js": "npx tsup",
    "build": "npm run build:js && npm run build:css",
    "dev": "npm run build -- --watch",
    "lint": "npx eslint src/**/*.ts* --fix",
    "test": "npx vitest run",
    "test:watch": "npx vitest"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [],
  "author": "Igor TÃºllio <igortullio@hotmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.1",
    "@radix-ui/react-checkbox": "^1.0.1",
    "tailwind-variants": "^0.3.0",
    "phosphor-react": "^1.4.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@igortullio-ui/eslint-config": "*",
    "@igortullio-ui/ts-config": "*",
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
